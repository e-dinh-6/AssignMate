{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": "off",
    "no-shadow": "off",
    "no-use-before-define": "off"
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  }
}
